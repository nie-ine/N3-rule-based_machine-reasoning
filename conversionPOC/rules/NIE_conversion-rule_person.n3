#

{?person
#	a human:Person;
	human:hasFamilyName ?familyNameValue;
	human:hasGivenName ?givenNameValue;
#	human:hasBirthDate
#	human:hasDeathDate
	knora-base:hasPermissions ?personPermissions.

?familyNameValue
	a knora-base:TextValue;
	knora-base:valueHasString ?familyName;
	knora-base:hasPermissions ?familyNamePermissions.

?givenNameValue
	a knora-base:TextValue;
	knora-base:valueHasString ?givenName;
	knora-base:hasPermissions ?givenNamePermissions.

